  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "npx ts-node-dev --respawn --watch -P ./tsconfig.json src/index.ts",
    "test": "npx jest --runInBand",
    "lint": "eslint . --ext .ts",
    "vercel:dev": "vercel dev",
    "test:docker": "docker compose -f docker-compose.test.yml up -d &&  jest --runInBand && docker compose -f docker-compose.test.yml down -v",
    "migrations:run": "npx sequelize db:migrate",
    "migrations:delete": "npx sequelize db:migrate:undo:all",
    "migrations:revert": "npx sequelize db:migrate:undo --name"
  },
